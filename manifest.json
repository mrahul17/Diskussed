{
  "manifest_version": 2,
  "name": "Diskussions",
  "version": "0.1.0",
  "browser_action": {
    "default_title": "Diskussions",
    "default_popup": "popup.html"
  },
  "permissions": ["webNavigation", "storage", "tabs"],
  "background": {
    "scripts": [ "background.js" ]
  },
  "icons": {
    "32": "icons/icon-48.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "strict_min_version": "50.0"
    }
  }
}